{
  "extends": [
    "airbnb/base",
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:react/recommended",
    "plugin:jsx-a11y/recommended"
  ],
  "plugins": [
    "react",
    "import",
    "jsx-a11y"
  ],
  "rules": {
    "no-console": "error",
    "react/prop-types": 0,
    "indent": [
      "error",
      2
    ],
    "linebreak-style": 1,
    "quotes": [
      "error",
      "double"
    ],
    "semi": "off",
    "react/jsx-uses-react": "off",
    "react/react-in-jsx-scope": "off",
    "jsx-a11y/no-autofocus": [
      2,
      {
        "ignoreNonDOM": true
      }
    ]
  },
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "env": {
    "es2022": true,
    "browser": true,
    "node": true
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  }
}
